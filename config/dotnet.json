{
	"SingleThread": false,
	"Encoding": "UTF-8",
	"Patterns": [
		{
			"Pattern": "^.*[^\\@\\$\\\"\\\\](%{rxStringLiteral}).*$",
			"Options": "NonBacktracking",
			"Replacement": "$1",
			"ReplaceOnly": true
		},
		{
			"Pattern": "^(%{rxStringLiteral}).+$",
			"Options": "NonBacktracking",
			"Replacement": "$1",
			"ReplaceOnly": true
		},
		{
			"Pattern": "^.*(_+|gettext[nd]?|Translate(Plural)?|GetTerm|Std(In|OutErr)|Description|DisplayText)\\s*\\(\\s*(%{rxStringLiteral}).*$",
			"Options": "NonBacktracking",
			"Replacement": "$4"
		},
		{
			"Pattern": "^.*\\(.*(Example|ErrorMessage)\\s*\\=\\s*(%{rxStringLiteral}).*$",
			"Options": "NonBacktracking",
			"Replacement": "$2"
		},
		{
			"Pattern": "^.*ExitCode[^\\(]*\\(\\d+\\s*,\\s*(%{rxStringLiteral}).*$",
			"Options": "NonBacktracking",
			"Replacement": "$1"
		},
		{
			"Pattern": "^.*[^\\@\\$\\\"\\\\](%{rxStringLiteral}).*wan24I8NTool\\:include.*$",
			"Options": "NonBacktracking",
			"Replacement": "$1"
		}
	],
	"FileExtensions": [
		".cs",
		".razor",
		".cshtml",
		".aspx",
		".cake",
		".vb"
	],
	"Exclude": [
		"*/obj/*"
	],
	"MergeOutput": false,
	"FailOnError": false,
	"Merge": false
}