{
	"SingleThread": false,
	"Encoding": "UTF-8",
	"Patterns": [
		{
			"Pattern": "^.*[^\\@\\$\\\"\\\\](%{rxStringLiteral}).*$",
			"Options": "NonBacktracking",
			"Replacement": "$1",
			"ReplaceOnly": true
		},
		{
			"Pattern": "^(%{rxStringLiteral}).+$",
			"Options": "NonBacktracking",
			"Replacement": "$1",
			"ReplaceOnly": true
		},
		{
			"Pattern": "^.*(_+|gettextn?)\\s*\\(\\s*(%{rxStringLiteral}).*$",
			"Options": "NonBacktracking",
			"Replacement": "$4"
		},
		{
			"Pattern": "^.*[^\\@\\$\\\"\\\\](%{rxStringLiteral}).*wan24I8NTool\\:include.*$",
			"Options": "NonBacktracking",
			"Replacement": "$1"
		}
	],
	"FileExtensions": [
		".cpp",
		".h"
	],
	"MergeOutput": false,
	"FailOnError": false,
	"Merge": false
}